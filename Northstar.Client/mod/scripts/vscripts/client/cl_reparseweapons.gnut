global function Cl_WeaponReparse

void function Cl_WeaponReparse()
{
    thread Cl_WeaponReparse_Threaded()
}

void function Cl_WeaponReparse_Threaded()
{
    while( !IsValid( GetLocalClientPlayer() ))
        WaitFrame()

    entity player = GetLocalClientPlayer()

    player.ClientCommand("weapon_reparse")
}